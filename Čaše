#include <stdio.h>
#include <string.h>
#include <ctype.h>

#ifndef DEBUG
#define DEBUG(...) printf(__VA_ARGS__)
#endif

void swap(int *x, int *y) {
  int tmp = *x;
  *x = *y;
  *y = tmp;
}

int main() {

  int glass[3], n;

  for (int i = 0; i < 3; ++i) {
    scanf("%d", glass+i);
  }

  scanf("%d", &n);

  for (int i = 0; i < n; ++i) {
    int c1, c2;
    scanf("%d%d", &c1, &c2);
    swap(glass+c1-1, glass+c2-1);
  }

  for (int i = 0; i < 3; ++i) {
    if (*(glass+i)) printf("%d", i+1); //ako bi pisali (glass+1) provjerava da li postoji pointer i vraÄ‡a 123
  }

  return 0;
}
